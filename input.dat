import psi4fockci
from psi4fockci import sf_cas
import numpy as np
import psi4

# setting up molecule
n2 = psi4.core.Molecule.from_string("""
0 7
N 0 0 0
N 0 0 2.5
symmetry c1
""")
# set target charge and multiplicity
charge = 0
multiplicity = 1

# running the spin-flip calculation
opts = {'basis': 'cc-pvdz', 'NUM_ROOTS': 4, 'reference': 'rohf'}
psi4.set_options(opts)

e, ci_wfn, scf_wfn = sf_cas( charge, multiplicity, n2, conf_space="", add_opts=opts, return_rohf_wfn=True, return_ci_wfn=True)


